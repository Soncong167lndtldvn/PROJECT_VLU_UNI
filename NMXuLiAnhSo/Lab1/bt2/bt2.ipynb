    "from skimage import io\n",
    "image = io.imread('bird.png')\n",
    "swapped_image = image.copy()\n",
    "swapped_image[:, :, 0] = image[:, :, 2]\n",
    "swapped_image[:, :, 2] = image[:, :, 0]\n",
    "io.imsave('swapped_colors.png', swapped_image)"
